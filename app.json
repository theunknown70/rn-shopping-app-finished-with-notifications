{
  "expo": {
    "name": "ShopNow",
    "slug": "shopping-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/shopicon.png",
    "description": "It is an E-Commerce Mobile App particularly for buying and selling of products.",
    "primaryColor": "#FFA500",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theunknown70.shoppingapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/shopicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "package": "com.theunknown70.shoppingapp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
